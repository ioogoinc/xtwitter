<div id="<%= dom_id tweet %>">
  <p>
    <strong>User:</strong>
    <%= tweet.user_id %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= tweet.body %>
  </p>

  <p>
    <strong>Type:</strong>
    <%= tweet.type %>
  </p>

  <p>
    <strong>Parent tweet:</strong>
    <%= tweet.parent_tweet_id %>
  </p>

</div>
